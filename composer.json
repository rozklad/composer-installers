{
    "name": "rozklad/composer-installers",
    "description": "Custom composer installers",
    "authors": [
        {
            "name": "rozklad",
            "email": "jan.rozklad@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rozklad\\ComposerInstallers\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        },
        {
            "type": "composer",
            "url": "http://packages.rozklad.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cartalyst/composer-installers": ">=1.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
